#include <stdio.h>
struct phanso
{
  int tu;
  int mau;
};
int
main ()
{

  struct phanso a;
  printf ("nhap phan so thu 1\n");
  printf ("nhap tu so 1:");
  scanf ("%d", &a.tu);
  printf ("nhap mau so 1:");
  scanf ("%d", &a.mau);
  struct phanso b;
  printf ("nhap phan so thu 2\n");
  printf ("nhap tu so 2:");
  scanf ("%d", &b.tu);
  printf ("nhap mau so 2:");
  scanf ("%d", &b.mau);
  printf ("tong la:%d/%d  \n", (a.tu*b.mau+a.mau*b.tu),(a.mau*b.mau));
  printf ("hieu la:%d/%d  \n", (a.tu*b.mau-a.mau*b.tu),(a.mau*b.mau));
  printf ("tich la:%d/%d \n", (a.tu*b.tu),(a.mau*b.mau));
  printf ("tich la:%d/%d \n", (a.tu*b.mau),(a.mau*b.tu));
  return 0;
}