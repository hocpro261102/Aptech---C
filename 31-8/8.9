#include <stdio.h>
void nhapmang(int a[],int n)
{
    for(int i=0;i<n;i++)
        {
            printf("nhap phan tu thu %d:",i+1);
            scanf("%d",&a[i]);
        }
}
void xuatmang(int a[],int n)
{
    for(int i=0;i<n;i++)
        {
            
            printf("%d \t",a[i]);
        }
}
void hoandoi(int a[],int n)
{
 int i,j,tam;
 for(i=0;i<n+1;i++)
    for(j=n-1;j>i;j--)
        if(a[j]<a[j-1])
            {
            tam=a[j];
            a[j]=a[j-1];
            a[j-1]=tam;
            }
}
int main()
{
    int a[100];
    int n;
    printf("nhap so phan tu cua mang:");
    scanf("%d",&n);
    nhapmang(a,n);
    xuatmang(a,n);
    hoandoi(a,n);
    printf("\n");
    xuatmang(a,n);
    return 0;
}